
CREATE TABLE salaryTimeLine(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    employeeId INTEGER,
    createDate TEXT,
    baseSalary INTEGER,
    housingSalary INTEGER,
    performanceSalary INTEGER,
    FOREIGN KEY(employeeId) REFERENCES employee(id)
);
insertSalaryTimeLine:
INSERT INTO salaryTimeLine(employeeId, createDate,baseSalary, housingSalary,performanceSalary) VALUES?;

deleteSalaryByEmployeeId:
DELETE FROM salaryTimeLine WHERE employeeId = ?;



